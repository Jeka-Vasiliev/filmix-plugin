{
    "defaultSeverity": "warning",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [
            true,
            "single",
            "avoid-escape",
            "avoid-template",
            "jsx-double"
        ],
        "semicolon": [
            true,
            "always"
        ],
        "arrow-parens": [
            true,
            "ban-single-arg-parens"
        ],
        "object-literal-sort-keys": [
            true,
            "ignore-case",
            "match-declaration-order"
        ],
        "interface-name": false
    },
    "rulesDirectory": []
}