{
    "compilerOptions": {
        "noImplicitAny": true,
        "removeComments": true,
        "moduleResolution": "node",
        "jsx": "react",
        "jsxFactory": "h",
        "target": "esnext",
        "allowSyntheticDefaultImports": true,
        "strict": true
    },
    "awesomeTypescriptLoaderOptions": {
        "useBabel": true,
        "useCache": true
    },
    "include": [
      "src/**/*",
      "typings/**/*"
    ]
}